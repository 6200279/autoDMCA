[run]
source = app
omit = 
    */tests/*
    */migrations/*
    */venv/*
    */env/*
    */__pycache__/*
    */node_modules/*
    app/main.py
    app/startup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = AutoDMCA Backend Coverage Report

[xml]
output = coverage.xml

# Coverage thresholds
fail_under = 80
precision = 2
show_missing = True
skip_covered = False
sort = Cover